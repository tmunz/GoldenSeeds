{
  "meta": {
    "name": "overlapping-triangles"
  },
  "grid": {
    "type": "polar",
    "items": "377",
    "angle": "n * 360 / goldenRatio",
    "distance": "Math.pow(n, 1 / goldenRatio)"
  },
  "background": {
    "type": "circle",
    "color": "none"
  },
  "drawer": {
    "type": "regular-shape",
    "style": "filled",
    "corners": "3",
    "color": "gold",
    "angle": "n * 360 / goldenRatio - 90",
    "size": "0.5 + Math.max(Math.pow(n, 1 / 4) * goldenRatio, 1)",
    "ratio": "Math.min(0.4, Math.pow(1 / n, 2 / 5))",
    "cutRatio0": "0",
    "cutRatio1": "0"
  }
}